@model List<Product>
@*
    _DashboardLowStockProductsComponentPartial
*@

<div class="col-lg-4 col-md-6 col-sm-12">
    <div class="card activities">
        <div class="header">
            <h2>Stok Durumu Kritik Olan Ürünler</h2>
        </div>
        <div class="body">
            <div class="streamline b-l b-accent">
                @foreach (var item in Model)
                {
                    <div class="sl-item">
                        <div class="sl-content">
                            <div class="text-muted-dk">@item.ProductName: @item.StockQuantity Adet</div>
                            <p><a href="" class="text-info"></a></p>
                        </div>
                    </div>
                }



            </div>
        </div>
    </div>
</div>